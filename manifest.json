{
  "manifest_version": 2,
  "name": "SES",
  "version": "0.0.0",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action" : {
    "default_title" : "Suspicious Email Submitter",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions" : [
    "*://*/*",
    "tabs"
  ]
}
