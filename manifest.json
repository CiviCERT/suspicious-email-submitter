{
  "manifest_version": 2,
  "name": "Suspicious Email Submitter",
  "short_name": "SES",
  "version": "1.0.1",
  "default_locale": "en",
  "background": {
    "page": "background.html"
  },
  "browser_action" : {
    "default_title" : "Suspicious Email Submitter",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions" : [
    "*://*/*",
    "tabs"
  ]
}
