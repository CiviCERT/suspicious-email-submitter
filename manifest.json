{
  "manifest_version": 2,
  "name": "Suspicious Email Submitter",
  "short_name": "SES",
  "version": "1.5.0",
  "default_locale": "en",
  "background": {
    "page": "background.html"
  },
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "browser_action" : {
    "default_title" : "Suspicious Email Submitter",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon_16.png",
      "32": "icon_32.png",
      "48": "icon_48.png",
      "128": "icon_128.png"
    }
  },
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false,
    "browser_style": true
  },
  "permissions" : [
    "*://*/*",
    "tabs",
    "messagesRead"
  ]
}
